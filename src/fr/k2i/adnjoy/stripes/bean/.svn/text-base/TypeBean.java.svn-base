package fr.k2i.adnjoy.stripes.bean;

import fr.k2i.adnjoy.business.ad.Type;

public class TypeBean {
	private Long id;
	private String code;
	private String name;
	public TypeBean(Type type) {
		id = type.getId();
		code = type.getCode();
		name = type.getName();
	}
	public TypeBean() {
		// TODO Auto-generated constructor stub
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

}
