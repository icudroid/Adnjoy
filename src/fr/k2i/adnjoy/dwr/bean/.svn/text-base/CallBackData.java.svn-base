package fr.k2i.adnjoy.dwr.bean;

public class CallBackData {
   
    public CallBackData() {

        err = new ErrorDwr();
        err.setErrno(0);
    }

    private ErrorDwr err;

    private Object data;

    /**
     * @return the err
     */
    public ErrorDwr getErr() {

        return err;
    }

    /**
     * @param err
     *            the err to set
     */
    public void setErr(ErrorDwr err) {

        this.err = err;
    }

    /**
     * @return the data
     */
    public Object getData() {

        return data;
    }

    /**
     * @param data
     *            the data to set
     */
    public void setData(Object data) {

        this.data = data;
    }
}
